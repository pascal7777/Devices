                    <option value="I">
                        <option value="IIa">
                            <option value="IIb">
                            <option value="III">
                                <option value="IV">
                                    <option value="NA">
                </datalist>
            </div>
        </div>
        <% } %>


    <% if(product.type === "IVD" ) {%>
