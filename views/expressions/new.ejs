<% layout('layouts/boilerplate')%>

<div class="row">
 
            <h1 class="text-center">Enter new Source for:</h1>

            <h2 class="text-center">Product: <%= product.name%>  </h2>
                <h3 class="text-center"><%= product.cat%></h3> 
            <h3 class="text-center"><%= product.type%></h3>
                      
<form action="/products/<%=product._id%>/expressions" method="POST" novalidate class = "validated-form" enctype="multipart/form-data">
   
    <br>
    <div class="row">  
                <div class="col-sm-3">
                <div class="mb-3">
                        <label class="form-label" for="supplier">Supplier</label>
                        <input class="form-control" type="text" id="supplier" name="supplier" required>
                </div>
                </div>
                <div class="col-sm-3">
                <div class="mb-3">
                        <label class="form-label" for="supCountry">Sales/Distribution</label>
                        <input class="form-control" type="text" id="supCountry" name="supCountry" required>
                </div>
                </div>
                <div class="col-sm-3">
                <div class="mb-3">
                    <label class="form-label" for="gpuRef">GPU Case Reference</label>
                    <input class="form-control" type="text" id="gpuRef" name="gpuRef" required>
                </div>
                 </div>
                <div class="col-sm-3">
                <div class="mb-3">
                <label class="form-label" for="quotation">Supplier Quotation</label>
                <input class="form-control" type="text" id="quotation" name="quotation" required>
                </div>
                </div>
        </div>

        <div class="row">  
                <div class="col-sm-4">
                <div class="mb-3">
                    <label class="form-label" for="manufacturer">Manufacturer</label>
                    <input class="form-control" type="text" id="manufacturer" name="manufacturer" required>
                </div>  
                </div>  
                <div class="col-sm-4">
                <div class="mb-3">
                    <label class="form-label" for="mfAddress">City</label>
                    <input class="form-control" type="text" id="mfAddress" name="mfAddress" required>
                </div>  
                </div>
                <div class="col-sm-4">
                <div class="mb-3">
                    <label class="form-label" for="mfCountry">Country of Origin</label>
                    <input class="form-control" type="text" id="mfCountry" name="mfCountry" required>
                </div>  
                </div>  
        </div>  

        <div class="row">  
                <div class="col-sm-4">
                <div class="mb-3">
                    <label class="form-label" for="deviceModel">Device Model/Type (reference nr)</label>
                    <input class="form-control" type="text" id="deviceModel" name="deviceModel">
                </div> 
                </div> 
                <div class="col-sm-4">
                <div class="mb-3">
                    <label class="form-label" for="catnr">Catalogue nr</label>
                    <input class="form-control" type="text" id="catnr" name="catnr">
                </div> 
                </div> 
                <div class="col-sm-4">
                <div class="mb-3">
                    <label class="form-label" for="brandName">Brand Name</label>
                    <input class="form-control" type="text" id="brandName" name="brandName">
                </div> 
                </div> 
        </div> 
    
                    

                <br> 
                <hr style="height:5px; border: 1px dashed rgb(6, 91, 251);background-color:gray">
                <br>

    <div class="row">  
        <div class="col-3">
            <div class="mb-3">
                    <label class="form-label" for="iso9001" class="form-label">Manufacturer has a <br><strong>ISO 9001:2015 </strong>certificate (Y/N)</label>
                    <input class="form-control" list="yesno1" name="iso9001" id="iso9001">
                    <datalist id="yesno1">
                        <option value="Yes">
                        <option value="No">
                        <option value="NA">
                    </datalist>
            </div>
        </div>

        <div class="col-4">
                    <div class="mb-3">
                        <label class="form-label" for="body9001">Certifying Body<br>(name, country)</label>
                        <input class="form-control" type="text" id="body9001" name="body9001">
                    </div>  
        </div>  

        <div class="col-5">
                    <div class="mb-3">
                    <label class="form-label" for="iaf9001" class="form-label">Is the certification body listed <a href="https://www.iafcertsearch.org/search/certification-bodies?standards_id=ade51248-af9b-559f-aef1-f7329bdc20f3&standards_name=ISO%209001%3A2015" target="_blank">
                        here </a> from one of GHTF founding member countries (EU, USA, Japan, Canada, Australia) (Y/N) </label>
                    <input class="form-control" list="yesno9001" name="iaf9001" id="iaf9001">
                    <datalist id="yesno9001">
                        <option value="Yes">
                        <option value="No">
                        <option value="NA">
                    </datalist>
                    </div>
        </div>   
    </div>


    <div class="row">
            <div class="col-6">
                <div class="mb-3">
                            <label class="form-label" for="iso9001validTo">The ISO9001 Certificate is valid to:</label>
                            <input class="form-control" type="date" id="iso9001validTo" name="iso9001validTo" value="2024-01-01">
                </div>   
            </div>


            <div class="col-6">
                <div class="mb-3">
                        <label class="form-label" for="iso9001onLine" class="form-label">ISO9001 Certificate number verified online (Y/N)</label>
                        <input class="form-control" list="yesno2" name="iso9001onLine" id="iso9001onLine">
                        <datalist id="yesno2">
                            <option value="Yes, successfull">
                            <option value="No, not successfull">
                            <option value="Online check not available">
                        </datalist>
                </div>    
            </div>
    </div>    

<br>


<div class="row">  
    <div class="col-3">
            <div class="mb-3">
                    <label class="form-label" for="iso13485" class="form-label">Manufacturer has a <br><strong>ISO 13485:2016 </strong>certificate (Y/N)</label>
